{
  "name": "frontend",
  "version": "0.1.0",
  "dependencies": {
    "@babel/preset-env": "^7.24.7",
    "@emailjs/browser": "^4.3.3",
    "@swc/helpers": "^0.5.11",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "regenerator-runtime": "^0.14.1",
    "sortablejs": "^1.15.6",
    "web-vitals": "^2.1.4"
  },
  "overrides": {
    "nth-check": "2.1.1"
  },
  "scripts": {
    "cleanup": "if exist .parcel-cache rd /s /q .parcel-cache && if exist dist rd /s /q dist",
    "dev": "npm run cleanup && parcel ./index.html",
    "start": "parcel index.html",
    "prebuild": "npx update-browserslist-db@latest",
    "build": "parcel build index.html --dist-dir ./dist",
    "postbuild": "node postbuild.js",
    "postinstall": "npx update-browserslist-db@latest",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@eslint/js": "^9.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@vitest/coverage-v8": "^4.0.18",
    "buffer": "^6.0.3",
    "core-js": "^3.37.1",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "globals": "^15.6.0",
    "happy-dom": "^20.0.11",
    "os-browserify": "^0.3.0",
    "parcel": "^2.14.4",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "svgo": "^3.3.2",
    "vitest": "^4.0.18",
    "vm-browserify": "^1.1.2"
  }
}
